{ 
    "executionRoleArn": "arn:aws:iam::641710438856:role/ecsTaskExecutionRole", 
    "containerDefinitions": [ 
        { 
            "name": "sample-website", 
            "image": "<IMAGE1_NAME>", 
            "essential": true, 
            "portMappings": [ 
                { 
                    "hostPort": 5000, 
                    "protocol": "tcp", 
                    "containerPort": 5000 
                } 
            ] 
        } 
    ], 
    "requiresCompatibilities": [ 
        "FARGATE" 
    ], 
    "networkMode": "awsvpc", 
    "cpu": "256", 
    "memory": "512", 
    "family": "inhouse-test" 
} 
