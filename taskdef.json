{ 
    "executionRoleArn": "arn:aws:iam::641710438856:role/ecsTaskExecutionRole", 
    "containerDefinitions": [ 
        { 
            "name": "sample-website", 
            "image": "641710438856.dkr.ecr.us-west-2.amazonaws.com/inhouse-builds:latest", 
            "essential": true, 
            "portMappings": [ 
                { 
                    "hostPort": 80, 
                    "protocol": "tcp", 
                    "containerPort": 80 
                } 
            ] 
        } 
    ], 
    "requiresCompatibilities": [ 
        "FARGATE" 
    ], 
    "networkMode": "awsvpc", 
    "cpu": "256", 
    "memory": "512", 
    "family": "inhouse-builds" 
} 
